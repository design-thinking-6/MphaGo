<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.suyong.mphago">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_flat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_transparent"
        android:supportsRtl="true"
        android:theme="@style/Theme.MphaGo"
        android:usesCleartextTraffic="true">
        <activity android:name=".SearchResultActivity"></activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SettingActivity" />
        <activity android:name=".StudyActivity"></activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>